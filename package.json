{
  "name": "redstone-showroom",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "contracts": "make -C sway build && fuels typegen -i \"sway/dex/out/debug/dex-abi.json\" -i \"sway/token/out/debug/token-abi.json\" -o \"src/autogenerated\"",
    "fix": "rm -rf ./node_modules/@fuel-ts/providers/node_modules/graphql-request",
    "dev": "yarn fix && rm -rf .parcel-cache && parcel index.html",
    "build": "yarn fix && parcel build index.html"
  },
  "dependencies": {
    "@fuel-wallet/sdk": "^0.7.2",
    "@redstone-finance/fuel-connector": "0.2.1",
    "axios": "^1.2.2",
    "ethers": "^5.6.9",
    "graphql-request": "5.1.0",
    "parcel": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "assert": "^2.0.0",
    "buffer": "^5.5.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "alias": {
    "process": false
  }
}
